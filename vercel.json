{
  "version": 2,
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "^/$",
      "dest": "/api/index.html"
    },
    {
      "src": "^/(.*\\.php)$",
      "dest": "/api/$1"
    },
    {
      "src": "^/(.*)$",
      "dest": "/api/$1"
    }
  ]
}
